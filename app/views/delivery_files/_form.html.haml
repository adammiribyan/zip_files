= form_for @delivery_file do |f|
  -if @delivery_file.errors.any?
    #error_explanation
      %h2= "#{pluralize(@delivery_file.errors.count, "error")} prohibited this delivery_file from being saved:"
      %ul
        - @delivery_file.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :file
    = f.text_field :file
  .actions
    = f.submit 'Save'
