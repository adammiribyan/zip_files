<h1>Listing projects</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Reference zip file</th>
    <th>Source work zip file</th>
    <th>Delivery zip file</th>
    <th>Instuction zip file</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @projects.each do |project| %>
  <tr>
    <td><%= project.name %></td>
    <td><%= project.reference_zip_file %></td>
    <td><%= project.source_work_zip_file %></td>
    <td><%= project.delivery_zip_file %></td>
    <td><%= project.instuction_zip_file %></td>
    <td><%= link_to 'Show', project %></td>
    <td><%= link_to 'Edit', edit_project_path(project) %></td>
    <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Project', new_project_path %>
